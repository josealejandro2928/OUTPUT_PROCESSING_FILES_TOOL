package UnmappedModes
public
	system main
	end main;
	
	system implementation main.impl
		subcomponents
			proc: process proc.impl;
		properties
			Period => 1000 ms in modes (x1), 200 ms in modes (x2) applies to proc.t1;	
	end main.impl;
	
	process proc
		modes
			m1: initial mode;
			m2: mode;
	end proc;
	
	process implementation proc.impl
		subcomponents
			t1: thread t; -- No mapping!
			t2: thread t; -- No mapping!
	end proc.impl;
	
	thread t
		requires modes
			x1: initial mode;
			x2: mode;
		properties
			Period => 10000 ms in modes (x1), 2000 ms in modes (x2);
	end t;
end UnmappedModes;