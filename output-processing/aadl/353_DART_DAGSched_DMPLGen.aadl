	thread waypoint{
    	if (init@0 == 0) return;
      	waypointValid = 1;
      
      	if(id == 1) {
        	xt = lx@0 + 1;
        	yt = ly@0 + 1;
      	} else if(id == 2) {
        	xt = lx@0 - 1;
        	yt = ly@0 + 1;
      	} else if(id == 3) {
        	xt = lx@0 - 1;
        	yt = ly@0 - 1;
      	} else if(id == 4) {
        	xt = lx@0 + 1;
        	yt = ly@0 - 1;
      	}
    }**};
end waypoint_protector;

thread implementation waypoint_protector.i
end waypoint_protector.i;

process process_protector extends uav_process
	features
		out_threat_detection: out event data port;
		in_threat_action: in event data port;
end process_protector;

thread threat_detection_thread
	features
		out_threat_detection: out event data port;
		in_threat_action: in event data port;	
	flows
		detected_threat: flow source out_threat_detection;
		threat_reaction: flow sink in_threat_action;
	properties
		Period => 1 sec;
		Deadline => 1 sec;
		Zero_Slack_Scheduling::Criticality => 1;
		Compute_Execution_Time => 1 ms .. 1 ms;	
		Compute_Execution_Time => 1 ms .. 1 ms applies to in_threat_action;	
		Priority => 1;		
end threat_detection_thread;

process implementation process_protector.i extends uav_process.i
	subcomponents 
		waypoint_thread: refined to thread waypoint_protector.i {Classifier_Substitution_Rule=>Type_Extension;};
		threat_detection: thread threat_detection_thread;
	connections
		c10: port in_next_leader_position->waypoint_thread.in_next_leader_position;
		c11: port in_threat_action->threat_detection.in_threat_action;
		c12: port threat_detection.out_threat_detection->out_threat_detection;
end process_protector.i;

system protector extends node
	features
		in_next_leader_position: in event data port;
		out_threat_detection: out event data port;
		in_threat_action: in event data port;
end protector;

system implementation protector.i extends node.i
	subcomponents
		sw: refined to process process_protector.i {Classifier_Substitution_Rule=>Type_Extension;};
	connections
		c10: port in_next_leader_position->sw.in_next_leader_position;
		c11: port in_threat_action->sw.in_threat_action;
		c12: port sw.out_threat_detection->out_threat_detection;
