package DART_DAGSched_DMPLGen
public
	with DMPLProperties,Zero_Slack_Scheduling;	

	
thread collision_avoidance
	features
		in_nextposition: in event data port;
		middle_position_reached: in event data port;
		in_infront_available: in event data port;
		out_infront_available: out event data port;
		out_middle_position: out event data port;
		out_last_position: out event data port;
		last_position_reached: in event data port;
	flows
		ca_move_next_position: flow path in_nextposition->out_middle_position;
		ca_middle_position_reached: flow path middle_position_reached->out_infront_available {Zero_Slack_Scheduling::Synchronized_Flows =>(reference (ca_infront_now_available));} ;
		ca_infront_now_available: flow path in_infront_available->out_last_position
		{Zero_Slack_Scheduling::Synchronized_Flows =>(reference (ca_middle_position_reached));};
		ca_reached_last_position: flow sink last_position_reached;
	properties
		Compute_Execution_Time =>1 ms .. 1 ms applies to in_nextposition;
		Compute_Execution_Time =>2 ms .. 2 ms applies to middle_position_reached;
		Compute_Execution_Time =>2 ms .. 2 ms applies to in_infront_available;
		Compute_Execution_Time =>3 ms .. 3 ms applies to last_position_reached;
		Zero_Slack_Scheduling::Suspensions => (  [From_Port => reference (out_middle_position);
											 	  To_Port => reference (middle_position_reached); 
										 		  Minimum_Delay => 10 ms; 
												  Maximum_Delay => 14 ms;
												 ]
												,
												 [From_Port => reference (out_last_position);
												  To_Port => reference (last_position_reached); 
												  Minimum_Delay => 18 ms; 
												  Maximum_Delay => 22 ms;
												 ]
											  );
	--properties
	--	DMPLProperties::Directives => ("@BarrierSync","@PlatformController");
	annex dmpl{**                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
		@BarrierSync;
		thread collision_avoidance{
    		print_state(X,Y,id,x,y,xt,yt);
    		//my_sleep(1);
    		if(state == NEXT) {
      		//compute next point on route
      			if(x == xt && y == yt) return;
      			if(NEXT_XY()) return;
      			state = REQUEST;
    		} else if(state == REQUEST) {
      			//request the lock but only if it is free
      			if(EXISTS_LOWER(idp,lock[xp][yp]@idp != 0)) return;
      			lock[xp][yp] = 1;
      			state = WAITING;
    		} else if(state == WAITING) {
      			//grab the lock if we are the highest 
      			//id node to request or hold the lock
      			if(EXISTS_HIGHER(idp, lock[xp][yp]@idp != 0)) return;
      			state = MOVE;
    		} else if(state == MOVE) {
      			//now we have the lock on (x’,y’)
      			if(x != xp || y != yp) return;
      			state = NEXT;
    		}
    	}**};
end collision_avoidance;

thread implementation collision_avoidance.i
end collision_avoidance.i;

thread motion_controller
	features
		in_middleposition:       in  data port;
		middle_position_reached: out data port;
		in_lastposition:         in data port;
		last_position_reached:   out data port;
	flows
		ctl_move_to_middle: flow sink in_middleposition 
			{Zero_Slack_Scheduling::Synchronized_Flows=>(
				reference (ctl_reached_middle),
				reference (ctl_move_to_last),
				reference (ctl_reached_last)
			);};
		ctl_reached_middle: flow source middle_position_reached
			{Zero_Slack_Scheduling::Synchronized_Flows=>(
				reference (ctl_move_to_middle),
				reference (ctl_move_to_last),
				reference (ctl_reached_last)
			);};		
		ctl_move_to_last: flow sink   in_lastposition
			{Zero_Slack_Scheduling::Synchronized_Flows=>(
				reference (ctl_move_to_middle),
				reference (ctl_reached_middle),
				reference (ctl_reached_last)
			);};		
		ctl_reached_last: flow source last_position_reached
			{Zero_Slack_Scheduling::Synchronized_Flows=>(
				reference (ctl_move_to_middle),
				reference (ctl_reached_middle),
				reference (ctl_move_to_last)
			);};	
	properties
		Compute_Execution_Time => 1 ms .. 1 ms ;
		Period => 10 ms;
		Deadline => 10 ms;
		Priority => 3;
		annex dmpl{**
			@PlatformController;
			thread motion_controller {
				if (state != MOVE) return;
    			//-- if no waypoint set
    			if(x == xp && y == yp) return;
    			//-- if still moving
    			if(GRID_MOVE(xp,yp,1)) return;
    			//-- done moving update coordinates
    			lock[x][y] = 0; 
    			x = xp; y = yp;
  			}			                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
		**};
end motion_controller;

	
system node
	features
		in_infront_available: in event data port;
		out_infront_available: out event data port;
end node;

thread waypoint
	features
		out_nextposition: out event data port;		
end waypoint;

process uav_process
	features
		out_next_leader_position: out event data port;
		in_next_leader_position: in event data port;
		out_infront_available: out event data port;
		in_infront_available: in event data port;
end uav_process;

process implementation uav_process.i
	subcomponents
		motion_controller_thread: thread motion_controller in modes (moving);
		collision_avoidance_thread: thread collision_avoidance.i in modes (spatial_agreement);
		waypoint_thread: thread waypoint in modes (spatial_agreement);
	connections
		c1: port waypoint_thread.out_nextposition -> collision_avoidance_thread.in_nextposition in modes (spatial_agreement);
		--c2: port waypoint_thread.out_infront_available->out_infront_available;
		--c3: port in_infront_available->waypoint_thread.in_infront_available;
		c4: port collision_avoidance_thread.out_middle_position->motion_controller_thread.in_middleposition in modes (spatial_agreement, moving);
		c5: port collision_avoidance_thread.out_last_position->motion_controller_thread.in_lastposition in modes (spatial_agreement, moving);
		c6: port motion_controller_thread.middle_position_reached->collision_avoidance_thread.middle_position_reached in modes (spatial_agreement, moving);
		c7: port motion_controller_thread.last_position_reached->collision_avoidance_thread.last_position_reached in modes (spatial_agreement, moving);
		c8: port collision_avoidance_thread.out_infront_available -> out_infront_available in modes (spatial_agreement);
		c9: port in_infront_available->collision_avoidance_thread.in_infront_available in modes (spatial_agreement);
	modes
		spatial_agreement: initial mode;
		moving : mode;
	properties
		Zero_Slack_Scheduling::Criticality => 1 applies to collision_avoidance_thread;
		Zero_Slack_Scheduling::Criticality => 1 applies to waypoint_thread;
		Zero_Slack_Scheduling::Criticality => 1 applies to motion_controller_thread;
		Period => 100000 ms applies to collision_avoidance_thread;
	annex dmpl{**                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                           
		node uav_process_i {
  			//shared variables -- visible across nodes. 
  			//must be accessed via R() and W()
  			global bool lock [X][Y] = {
    			lock[x][y] = 1;
  			};
  
  			global unsigned char lx = 0,ly = 0; //leader's destination coordinates
  			global bool init = 0; //leader's destination has been initialized

  			//private variables -- local to the node.
  			//preserved across rounds
  			local unsigned char state = NEXT; //the state

			//current coordinates
  			local unsigned char input x ~ (0 <= x && x < X), input y ~ (0 <= y && y < Y);
  			local unsigned char xp = x, yp = y; //next coordinates
  			//final coordinates
  			local unsigned char input xt ~ (0 <= xt && xt < X), input yt ~ (0 <= yt && yt < Y);
  
  			local bool waypointValid = 0; //xt, yt are valid

			pure int expProp1Func() { return x < 5; }
  			pure int expProp2Func() { return y < 5; }
  
  			expect expProp1 : at_end => expProp1Func;
  			expect expProp2 : at_end => expProp2Func;

			pure int coverage()
  			{
    			return (GET_LAT() - GET_LNG()@0) > 0.9;
  			}

  			expect expProp3 : at_least 0.5 => coverage;
  
  			bool NEXT_XY () {
    			if(!waypointValid) return 1;
    			if(xt < 0 || xt >= X || yt < 0 || yt >= Y) return 1;

    			xp = x; yp = y;
    			if(x < xt) xp = x + 1;
    			else if(x > xt) xp = x - 1;
    			else if(y < yt) yp = y + 1;
    			else yp = y - 1;
    			return 0;
  			}
			@InitSim;
  			void StartingPosition ()
  			{
    			GRID_INIT();
    			GRID_PLACE(x, y, 1);
  			}

  			//the safety property checked at the beginning of each round
  			pure bool NoCollisions ()
  			{
    			//-- the main property
    			FORALL_DISTINCT_NODE_PAIR (id1,id2) {
      				if(x@id1 == x@id2 && y@id1 == y@id2) return false;
    			}
    
    			//-- supporting invariants for inductiveness

    			FORALL_NODE(idp) {
      				/** state has to be between 0 and 4, (x,y), (xf,yf), and (xp,yp)
          				have to be between 0 and X-1,Y-1 */
      				if(!(state@idp >= NEXT && state@idp <= MOVE)) return false;
      				if(!(x@idp >= 0 && x@idp < X)) return false;
      				if(!(y@idp >= 0 && y@idp < Y)) return false;
      				//if(!(xt@idp >= 0 && xt@idp < X)) return false;
      				//if(!(yt@idp >= 0 && yt@idp < Y)) return false;
      				if(!(xp@idp >= 0 && xp@idp < X)) return false;
      				if(!(yp@idp >= 0 && yp@idp < Y)) return false;
      				/** if we are in not in NEXT, then xp,yp != x,y */
      				if(!(state@idp == NEXT || x@idp != xp@idp || y@idp != yp@idp)) return false;
      				/** if we are in NEXT, REQUEST, WAITING or MOVE, we have lock on
          				x,y */
      				if(!((state@idp != NEXT && state@idp != REQUEST && state@idp != WAITING && state@idp != MOVE) ||
           				lock[x@idp][y@idp]@idp)) return false;
      				/** if we are in NEXT, WAITING or MOVE, then we have lock on x,y
          				and xp,yp */
      				if(!((state@idp != NEXT && state@idp != WAITING && state@idp != MOVE) ||
           				(lock[x@idp][y@idp]@idp && lock[xp@idp][yp@idp]@idp))) return false;
      				/** if we are not in NEXT, then x,y and xp,yp are
          				adjacent */
      				if(!(state@idp == NEXT || (((x@idp == xp@idp - 1) || (x@idp == xp@idp + 1)) ^ ((y@idp == yp@idp - 1) || (y@idp == yp@idp + 1))))) return false;
    			}
  
    			//-- the rest are supporting invariants. you only need them for
    			//-- plain induction. you can skip them (by uncommenting the
    			//-- following return statement) out if you are doing K-induction
    			//-- with K>=3.
    			//return true;
      
    			FORALL_DISTINCT_NODE_PAIR (id1,id2) {
      				/** if node1 is in MOVE/NEXT and has lower id than node2 which is
          				WAITING then they have different xp,yp */
      				if(!((state@id1 != NEXT && state@id1 != MOVE) || state@id2 != WAITING || id1 >= id2 || xp@id1 != xp@id2 || yp@id1 != yp@id2)) return false;
      				/** if two distinct nodes are in MOVE, they have different
          				(xp,yp) */
      				if(!(state@id1 != MOVE || state@id2 != MOVE ||                        
           				((xp@id1 != xp@id2 || yp@id1 != yp@id2) &&             
            			(xp@id1 != x@id2 || yp@id1 != y@id2) &&               
            			(x@id1 != xp@id2 || y@id1 != yp@id2) &&               
            			(x@id1 != x@id2 || y@id1 != y@id2)))) return false;
      				/** if node2 in WAITING, and node1 has lower id than node 2, then
          				node1.(x,y) != node2.(xp,yp) */
      				if(!(state@id2 != WAITING || id1 >= id2 || x@id1 != xp@id2 || y@id1 != yp@id2)) return false;
      				/** if node2 in MOVE, then node1.(x,y) != node2.(xp,yp) */
      				if(!((state@id2 != MOVE || x@id1 != xp@id2 || y@id1 != yp@id2) && (state@id1 != 4 || xp@id1 != x@id2 || yp@id1 != y@id2))) return false;
      				/** two distinct nodes have different (x,y) */
      				if(!(x@id1 != x@id2 || y@id1 != y@id2)) return false;
    			}
    
    			return true;
  			}

  			//-- guaranteed properties
  			require NoCollision => NoCollisions;
  		}**};
end uav_process.i;

processor node_processor
end node_processor;

system implementation node.i
	subcomponents
		sw: process uav_process.i;
		hw: processor node_processor;
	connections
		ca: port in_infront_available->sw.in_infront_available;
		cb: port sw.out_infront_available->out_infront_available;
	properties
		--Zero_Slack_Scheduling::Scheduling_Protocol => ZSRM applies to hw;
		Actual_Processor_Binding => (reference (hw)) applies to sw;
end node.i;

system leader extends node 
	features
		out_next_leader_position: out event data port;
		in_threat_detection: in event data port;
		out_threat_action: out event data port;		
end leader;

-- We want to express that there is a single computation that arrives not because of a port but because
-- of progression of time (time-triggered; e.g., periodically) and has two outputs.
-- We do this with the AADL construction flow.
-- A flow is a computation that takes input and produces output
-- below, we declare two flows but they refer to the same computation
-- we use the property Synchronized_Flows that these two flows refer to the same computation
thread waypoint_leader extends waypoint
	features
		out_next_leader_position: out event data port;
	flows
		leader_compute_next_waypoint: flow source out_next_leader_position
			{Zero_Slack_Scheduling::Synchronized_Flows =>(reference (move_to_next_leader_waypoint));};
		move_to_next_leader_waypoint: flow source out_nextposition
		    {Zero_Slack_Scheduling::Synchronized_Flows =>(reference (leader_compute_next_waypoint));};
	properties
		Period => 100000 ms;
		Deadline => 100000 ms;
