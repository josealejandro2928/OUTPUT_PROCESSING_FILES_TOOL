extern void print_int(int i);**};	
end main;

-- DMPL command line to generate:
-- assume that the generated dmpl has 
-- node uav{..}
-- dmplc --roles <node name>:<role name>:<number of nodes> ...
-- example : dmplc --roles node:leader:1:node:protector:1 
system implementation main.i
	subcomponents
		leader_node: system leader.i[1];
		protector_node: system protector.i[4];
	connections
		c100: port leader_node.out_next_leader_position->protector_node.in_next_leader_position {Connection_Set => ([src=>(1);dst=>(1);],[src=>(1);dst=>(2);]);} ;--{Connection_Pattern => ((One_To_All));};
		c101: port leader_node.out_infront_available->protector_node.in_infront_available {Connection_Set => ([src=>(1);dst=>(1);]);};
		c102: port protector_node.out_infront_available->protector_node.in_infront_available {Connection_Set => ([src=>(1);dst=>(2);]);};
		c103: port protector_node.out_threat_detection->leader_node.in_threat_detection {Connection_Set => ([src=>(1);dst=>(1);],[src=>(2);dst=>(1);]);};
		c104: port leader_node.out_threat_action->protector_node.in_threat_action{Connection_Set => ([src=>(1);dst=>(1);],[src=>(1);dst=>(2);]);};
	properties
		Zero_Slack_Scheduling::Scheduling_Protocol => ZSDAG;
		Zero_Slack_Scheduling::SubDeadlines=>(
			[
				From_Flow => reference (leader_node[1].sw.waypoint_thread.leader_compute_next_waypoint);
				To_Flow => reference (leader_node[1].sw.collision_avoidance_thread.ca_middle_position_reached);
				Deadline => 2010 ms;
			],
			[
				From_Flow => reference (leader_node[1].sw.waypoint_thread.leader_compute_next_waypoint);
				To_Flow => reference (protector_node[1].sw.collision_avoidance_thread.ca_middle_position_reached);
				Deadline => 2020 ms;
			],
			[
				From_Flow => reference (leader_node[1].sw.waypoint_thread.leader_compute_next_waypoint);
				To_Flow => reference (protector_node[2].sw.collision_avoidance_thread.ca_middle_position_reached);
				Deadline => 2030 ms;
			]
			);
		--Dispatch_Protocol => Periodic  applies to leader_node[1].sw.waypoint_thread;
		--Dispatch_Protocol => Aperiodic applies to leader_node[1].sw.collision_avoidance_thread;
		--Dispatch_Protocol => Periodic  applies to leader_node[1].sw.motion_controller_thread;
		--Dispatch_Protocol => Aperiodic applies to protector_node[1].sw.waypoint_thread;
		--Dispatch_Protocol => Aperiodic applies to protector_node[1].sw.collision_avoidance_thread;
		--Dispatch_Protocol => Periodic  applies to protector_node[1].sw.motion_controller_thread;
		--Dispatch_Protocol => Aperiodic applies to protector_node[2].sw.waypoint_thread;
		--Dispatch_Protocol => Aperiodic applies to protector_node[2].sw.collision_avoidance_thread;
		--Dispatch_Protocol => Periodic  applies to protector_node[2].sw.motion_controller_thread;
		DMPLProperties::Output_Dir => "outdir";
	    DMPLProperties::Grid_Size => 10;
        DMPLProperties::Map_Name => "small";
        DMPLProperties::Record_Camera_Pos => "0.0421 -0.2489 11.2217";
        DMPLProperties::Mission_Time => 240 sec;
        DMPLProperties::Expect_Log_Period =>0.2;
        DMPLProperties::Platform => "vrep-uav::::0.1";
        DMPLProperties::Initial_Parameter_Values => ("x","1","y","1","xt","8","yt","8") applies to leader_node.sw;
        DMPLProperties::Initial_Parameter_Values => ("x","2","y","2","xt","9","yt","9") applies to protector_node[1].sw;
        DMPLProperties::Initial_Parameter_Values => ("x","0","y","2","xt","7","yt","9") applies to protector_node[2].sw;
        DMPLProperties::Initial_Parameter_Values => ("x","0","y","0","xt","7","yt","7") applies to protector_node[3].sw;
        DMPLProperties::Initial_Parameter_Values => ("x","2","y","0","xt","9","yt","7") applies to protector_node[4].sw;
		-- to generate "echo --node uav_process_i --role $6 --var_<varname> <value> --var_<varname> <value>"
		-- example with the current property: "echo --node xxx --role xxx --var_x 1 --var_y 2"
end main.i;	

